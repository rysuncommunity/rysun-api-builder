  },
  "dependencies": {
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/common": "^10.2.1",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.2.1",
    "@nestjs/graphql": "^12.0.8",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/swagger": "^7.1.8",
    "@prisma/client": "^5.2.0",
